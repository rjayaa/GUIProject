<Page x:Class="BackupProj.InsertSelectionPage"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:BackupProj"
      mc:Ignorable="d" 
      d:DesignHeight="450" d:DesignWidth="800"
      Title="InsertSelectionPage">

    <StackPanel Background="White">
        <Grid Margin="0 30 0 0">
            <Grid.ColumnDefinitions>
                <ColumnDefinition Width="*"/>
                <ColumnDefinition Width="*"/>


            </Grid.ColumnDefinitions>

            <Button Content="Insert Barang" Width="200" Padding="7" HorizontalAlignment="Center" Margin="0,80,0,0" Height="32" VerticalAlignment="Top" Click="Button_Click"/>
            <Button Grid.Column="1" Content="Insert Komponen" Width="200" Padding="7" HorizontalAlignment="Left" Margin="20 80 0 0"/>
        </Grid>
        <Button Margin="0 50 0 0" HorizontalAlignment="Center" Grid.Column="2" Content="Informasi Transaksi" Width="200" Padding="7"/>

    </StackPanel>
</Page>
