<Page x:Class="BackupProj.LandingPage"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:BackupProj"
      mc:Ignorable="d" 
      d:DesignHeight="450" d:DesignWidth="800"
      Title="LandingPage">

    <StackPanel Background="White">
        <Button HorizontalAlignment="Left" Width="70" Margin="20 10 0 0" Grid.Column="0" Content="Import"/>
        <Button HorizontalAlignment="Left" Width="70" Margin="20 10 0 0" Grid.Column="1" Content="Insert" Click="Button_Click" Height="20" VerticalAlignment="Top" />
        
    </StackPanel>
</Page>
